func calcular(a, b, c) {
    x = a + b;
    y = x * c;
    z = y / 2;
    return z;
}

func media(a, b) {
    soma = a + b;
    resultado = soma / 2;
    return resultado;
}

func formula(x) {
    a = x * 2;
    b = a + 5;
    c = b ^ 2;
    d = c - 10;
    return d;
}

func chamarOutra(p) {
    resultado = quadrado(p);
    return resultado + 1;
}

func quadrado(n) {
    return n * n;
}

